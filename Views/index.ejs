<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>
    <h1><%= title %></h1>
    <ul>
      <% if (users && users.length > 0) { %>
        <% users.forEach(user => { %>
          <li>
            ID: <%= user.id %>, Username: <%= user.username %>, Message: <%= user.message %>
          </li>
        <% }); %>
      <% } else { %>
        <li>No users found.</li>
      <% } %>
    </ul>
    <nav>
        <a href="/new">New message</a>
    </nav>
</body>
</html>
